@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
    <link rel="manifest" href="/manifest.json">
    <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#e35c56">
    <meta name="apple-mobile-web-app-title" content="Mark and Eleanor 2019">
    <meta name="application-name" content="Mark and Eleanor 2019">
    <meta name="theme-color" content="#e35c56">
    <meta name="description" content="Mark and Eleanor are tying the knot! Join us on the 11th of May, 2019 to celebrate their marriage.">
    <meta name="og:image" content="@Url.AbsoluteContent("/images/bannerHero2.jpg")">

    <script type="application/ld+json">
        [{
        "@@context": "http://schema.org",
        "@@type": "Event",
        "name": "Mark and Eleanor's Wedding Ceremony",
        "startDate": "2019-05-11T16:30+02:00",
        "location": {
        "@@type": "Place",
        "name": "Parish Church of St Peter in Chains, Birżebbuġa",
        "address": {
        "@@type": "PostalAddress",
        "streetAddress": "Triq Birżebbuġa",
        "addressLocality": "Birżebbuġa",
        "postalCode": "BBG 2031",
        "addressCountry": "MT"
        }
        },
        "endDate": "2019-05-11T17:30+02:00"
        },
        {
        "@@context": "http://schema.org",
        "@@type": "Event",
        "name": "Mark and Eleanor's Wedding Reception",
        "startDate": "2019-05-11T18:00+02:00",
        "location": {
        "@@type": "Place",
        "name": "Chateau Buskett",
        "address": {
        "@@type": "PostalAddress",
        "streetAddress": "The Buskett Roadhouse",
        "addressLocality": "Buskett, l/o Siggiewi",
        "postalCode": "SDW 4616",
        "addressCountry": "MT"
        }
        },
        "endDate": "2019-05-12T01:30+02:00"
        }]
    </script>

    <title>Mark and Eleanor 2019</title>

    <link href="https://fonts.googleapis.com/css?family=Alex+Brush|Poppins|Roboto+Mono|Roboto+Slab" rel="stylesheet">

    <environment names="Development">
        <link href="~/lib/normalize/normalize.css" rel="stylesheet" />
        <link href="~/css/style.css" rel="stylesheet" />
    </environment>
    <environment names="Staging,Production">
        <link href="~/css/style.min.css" rel="stylesheet" asp-append-version="true" />
    </environment>

    @Html.Raw(JavaScriptSnippet.FullScript)
</head>
<body>
    <div class="wrapper">
        @RenderBody()
    </div>


    <environment names="Development">
        <script src="~/lib/jquery/jquery.js"></script>
        <script src="~/lib/moment.js/moment.js"></script>
        <script src="~/lib/moment-timezone/moment-timezone-with-data-2012-2022.js"></script>
        <script src="~/lib/jquery.countdown/jquery.countdown.js"></script>
        <script src="~/js/site.js"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="~/js/site.min.js" asp-append-version="true"></script>
    </environment>
    @RenderSection("Scripts", required: false)
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
                m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-75438054-4', 'auto');
        ga('send', 'pageview');
    </script>
</body>
</html>
